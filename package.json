{
  "name": "cineplus",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "clean": "cd android && ./gradlew clean && cd ..",
    "android:dev": "cross-env APP_ENV=development npx  expo run:android --variant=developmentDebug",
    "android:staging": "cross-env APP_ENV=staging npx expo run:android --variant=stagingDebug",
    "android:preprod": "cross-env APP_ENV=preprod npx expo run:android --variant=preprodDebug",
    "android:prod": "cross-env APP_ENV=production npx expo run:android --variant=productionRelease"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "expo": "~54.0.31",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-expo": "^10.0.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.0.0",
    "prettier": "^3.7.4"
  }
}
